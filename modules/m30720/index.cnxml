                        <m:mo stretchy="false">=</m:mo>
                        <m:mi>ρ</m:mi>
                      </m:mrow>
                      <m:mfrac>
                        <m:msup>
                          <m:mi>v</m:mi>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:mstyle>
                        </m:msup>
                        <m:mn>2</m:mn>
                      </m:mfrac>

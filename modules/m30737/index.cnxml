                            <m:mi>Q</m:mi>
                            <m:mfenced open="" close="">
                              <m:mrow>
                                <m:mn>1,6</m:mn>
                                <m:mrow>
                                  <m:mi>H</m:mi>
                                  <m:mo stretchy="false">+</m:mo>
                                  <m:mstyle fontstyle="italic">
                                    <m:mrow>
                                      <m:mtext>fL</m:mtext>
                                    </m:mrow>
                                  </m:mstyle>
                                </m:mrow>
                              </m:mrow>
                            </m:mfenced>
                          </m:mrow>

                </m:mstyle>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{N rSub { size 8{1} } = {  { left [0,3 left (1+ ital "BL" right )v+0,"003"Q left (H+1,8 ital "fL" right ) right ]}  over  {η} } } {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para id="id5797184">Đối với các băng tải đứng và dốc đứng:</para>
        <para id="id5797190">
          <m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mrow>
                      <m:msub>
                        <m:mi>N</m:mi>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:mn>1</m:mn>
                          </m:mrow>
                        </m:mstyle>
                      </m:msub>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mfrac>
                        <m:mfenced open="[" close="]">
                          <m:mrow>
                            <m:mn>0,</m:mn>
                            <m:mtext>07</m:mtext>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>Bv</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                            <m:mrow>
                              <m:mfenced open="" close="">
                                <m:mrow>
                                  <m:mrow>
                                    <m:mi>H</m:mi>
                                    <m:mo stretchy="false">+</m:mo>
                                    <m:mn>4,3</m:mn>
                                  </m:mrow>
